# Copy this file to .env.local for local development.
# Do NOT commit real secrets; use .env.local for local values.

# API base and payments
VITE_API_BASE_URL=/api/v1
VITE_PAYSTACK_PUBLIC_KEY=

# Media / Cloudinary (staging)
VITE_MEDIA_PROVIDER=cloudinary
VITE_CLOUDINARY_CLOUD_NAME=<CLOUD_NAME>
# Optional: override delivery base
VITE_CLOUDINARY_BASE_URL=https://res.cloudinary.com/<CLOUD_NAME>/
# Responsive max width for srcset building
VITE_MEDIA_MAX_WIDTH=1600

# Maps (from SCO-001)
VITE_GOOGLE_MAPS_API_KEY=
